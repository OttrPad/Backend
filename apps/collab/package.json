{
  "name": "collab",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e ../../.env -- nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "dev:no-watch": "dotenv -e ../../.env -- ts-node src/app.ts",
    "start": "dotenv -e ../../.env -- node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:broadcasting": "node tests/run-tests.js",
    "test:verbose": "jest --verbose",
    "test:integration": "jest --testPathPattern=tests/.*\\.test\\.ts --runInBand"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@packages/supabase": "workspace:*",
    "@supabase/supabase-js": "^2.53.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.7",
    "socket.io": "^4.8.1",
    "y-protocols": "^1.0.6",
    "y-websocket": "^1.5.4",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.x",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.2",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^10.0.0",
    "jest": "^29.5.0",
    "nodemon": "^3.0.1",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  }
}
