{
  "name": "core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e ../../.env -- nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "dev:no-watch": "dotenv -e ../../.env -- ts-node src/app.ts",
    "start": "dotenv -e ../../.env -- node dist/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@google/genai": "^1.19.0",
    "@packages/supabase": "workspace:*",
    "@supabase/supabase-js": "^2.53.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.23",
    "@types/node": "^18.19.121",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.2",
    "jest-environment-node": "^29.6.1"
  }
}
