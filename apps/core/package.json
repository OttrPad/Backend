{
  "name": "core",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "dotenv -e ../../.env -- ts-node src/app.ts",
    "dev:watch": "dotenv -e ../../.env -- nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "start": "dotenv -e ../../.env -- node dist/app.js"
  },
  "dependencies": {
    "@packages/supabase": "workspace:*",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.23",
    "@types/node": "^18.x",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3"
  }
}
