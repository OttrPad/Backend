{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["SUPABASE_URL", "SUPABASE_KEY"],
  "globalDependencies": ["**/.env", "**/.env.*"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "inputs": ["$TURBO_DEFAULT$", ".env", ".env.*"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
