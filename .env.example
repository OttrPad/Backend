# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret

# Service URLs
CORE_SERVICE_URL=http://localhost:3001
FRONTEND_URL=http://localhost:3000

# API Configuration
API_PORT=4000
CORE_PORT=3001
COLLABORATION_HTTP_PORT=5002
EXE_PORT=4004
NODE_ENV=development

# Service-to-Service Authentication
GATEWAY_SHARED_SECRET=your-gateway-shared-secret-min-32-chars

# AI Service Configuration
GEMINI_API_KEY=your-gemini-api-key

# Execution Service Configuration
# Enable stateful Python sessions (persist variables per room between exec calls)
EXE_STATEFUL=true

# Docker Configuration (for exe service - production only)
DOCKER_HOST=unix:///var/run/docker.sock
